<h1>Coupon Form</h1>
<%= form_tag coupons_path do %>
    <%= label_tag "coupon_code", "Coupon code: " %>
    <%= text_field_tag :"coupon[coupon_code]", nil, id: "coupon_code" %> <br>
    <%= label_tag "store", "Store: "%>
    <%= text_field_tag :"coupon[store]", nil, id: "store"  %> <br>
    <br>
    <%= submit_tag "Submit Coupon" %>
<% end %>